	<model_units>
		<unit name="Employees">
			<eqn/>
			<alias>employee</alias>
		</unit>
		<unit name="Dimensionless">
			<eqn>1</eqn>
			<alias>dmnl</alias>
			<alias>unitless</alias>
		</unit>
		<unit name="Months">
			<eqn/>
			<alias>mo</alias>
			<alias>month</alias>
		</unit>
	</model_units>
	<model>
		<variables>
			<stock name="Staff">
				<doc>Number of staff</doc>
				<eqn>10</eqn>
				<inflow>Hiring</inflow>
				<outflow>Turnover</outflow>
				<non_negative/>
				<units>Employees</units>
			</stock>
			<flow name="Hiring">
				<doc>hiring rate</doc>
				<eqn>Staffing_gap/Average_time_to_hire</eqn>
				<non_negative/>
				<units>Employees/Months</units>
			</flow>
			<flow name="Turnover">
				<doc>Staff turnover</doc>
				<eqn>turnover_fraction*Staff</eqn>
				<non_negative/>
				<units>Employees/Months</units>
			</flow>
			<aux name="Staffing gap">
				<doc>Number of employees needed </doc>
				<eqn>Staffing_goal-Staff</eqn>
			</aux>
			<aux name="Average time to hire">
				<eqn>2</eqn>
				<units>Months</units>
			</aux>
			<aux name="turnover\nfraction">
				<doc>Staff turnover fraction </doc>
				<eqn>.1</eqn>
				<units>Dimensionless/Months</units>
			</aux>
			<aux name="Staffing goal">
				<eqn>10+STEP(5, 2)</eqn>
			</aux>
			<isee:dependencies>
				<var name="Hiring">
					<in>Average_time_to_hire</in>
					<in>Staffing_gap</in>
				</var>
				<var name="Turnover">
					<in>Staff</in>
					<in>turnover_fraction</in>
				</var>
				<var name="Staffing_gap">
					<in>Staff</in>
					<in>Staffing_goal</in>
				</var>
			</isee:dependencies>
		</variables>
